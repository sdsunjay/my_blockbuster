<div id="ratings-index">
    <% @ratings.each_slice(4) do |rating| %>
        <div class="row">
            <% rating.each do |rating| %>
                <div class="col-xs-3 col-sm-3 col-md-3 col-lg-3">
                    <h3><%= rating.rating %></h3>
                </div>
            <% end %>
        </div>
    <% end %>
</div>
